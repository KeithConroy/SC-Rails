<h2 class="page-header">
  <%= @user.first_name %> <%= @user.last_name %>
  <small> - Student <%= @user.email %>
  <span class="pull-right">
    <%= link_to 'Edit', edit_organization_user_path(@user.organization_id, @user.id) %>
    <%= link_to 'Delete', organization_user_path(@user.organization_id, @user.id), method: :delete, data: { confirm: 'Are you sure?' } %>
  </span>
  </small>
</h2>

