<% @users.each do |user| %>
<tr>
  <td><%= link_to "#{user.first_name} #{user.last_name}", organization_user_path(user.organization_id, user.id) %></td>
  <td><%= user.email %></td>
  <td><%= status(user) %></td>
  <td class='icon'><%= link_to edit_organization_user_path(user.organization_id, user.id), class: "glyphicon" do %>
    <i class="fa fa-pencil-square-o fa-lg"></i>
  <% end %>
  </td>
  <td class='icon'><%= link_to organization_user_path(user.organization_id, user.id), method: :delete, data: { confirm: 'Are you sure you want to delete this user?' }, class: "glyphicon" do %>
    <i class="fa fa-trash-o fa-lg"></i>
  <% end %>
  </td>
</tr>
<% end %>